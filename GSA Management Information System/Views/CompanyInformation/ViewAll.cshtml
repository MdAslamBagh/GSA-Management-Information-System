@model IEnumerable<GSA_Management_Information_System.Models.CompanyInformation>
@{
    ViewBag.Title = "ViewAll";
    Layout = null;
}
<table class="table table-striped table-condensed" id="CompanyInformationsTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Company_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch_Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Tin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Postcode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Country)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_ContacNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Fax)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Web)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company_Dialogue)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Branch_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Branch_Code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Tin)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Postcode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_City)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Country)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_ContacNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Fax)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Web)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company_Dialogue)
                </td>
                <td>
                    <a class="btn btn-primary btn-sm" onclick="Edit('@Url.Action("AddOrEdit", "CompanyInformation", new { id=@item.CompanyID})')"><i class="fa fa-pencil fa-lg"></i></a>
                    <a class="btn btn-danger btn-sm" onclick="Delete('@Url.Action("Delete", "CompanyInformation", new { id=@item.CompanyID})')"><i class="fa fa-trash fa-lg"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>