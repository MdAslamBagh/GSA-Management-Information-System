                                        @*<div class="required">*@

                                            <input id="companydialouge" name="Company_Web" placeholder="Web" class="form-control" type="text">

                                        @*</div>*@
                                    </div>

                                </div>

                            </div>

                            <div class="col-sm-6">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-sm-4">
                                            <label for="Origin">
                                                Image Upload
                                                <span id="txtoriginname" style="color:#FF0000;">*</span>
                                            </label>
                                        </div>
                                        <div class="col-sm-8">
                                            @*<div class="required">*@

                                                <input type="file" name="Company_ImageUpload" required>

                                            @*</div>*@
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
