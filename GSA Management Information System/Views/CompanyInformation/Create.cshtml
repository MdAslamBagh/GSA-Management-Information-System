                                                </label>
                                            </div>
                                            <div class="col-sm-8">
                                                @*<div class="required">*@

                                                    <input id="companyaddress" name="Company_Address" placeholder="Company_Address" class="form-control" type="text">

                                                @*</div>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
