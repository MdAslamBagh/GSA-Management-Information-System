@section scripts{
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="/resources/demos/style.css">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    @*<script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>*@
    <!-- Load jQuery UI Main JS  -->



   
  


    <script type="text/javascript">
        jQuery.noConflict()(function ($) {

            $(function () {
                $("#DateFrom").datepicker();
                $("#DateTo").datepicker();

            });
            $(function () {
                //$("#mawbtextbox").hide()
                $("#datewise").hide()
                $("#freightpaymode").hide()
                $("#cargotypeitemwise").hide()
                $("#groupwise").hide()
                $("#agentwise").hide()
                $("#freighterwise").hide()
                $("#destinationwise").hide()
                $("#continentwise").hide();
                $("#btnsend").hide()

                $("#reportid").change(function () {
                    var reportid = $("#reportid").val();
                    if (reportid == null) {

                    }
                    else

                        $('#checkmasterdate').click(function () {
                            if ($('#checkmasterdate').is(':checked')) {
                                $("#checkmasterdate").val();
                                $("#checkflightdate").prop("checked", false);
                                //alert("Checkbox is checked.");
                            }
                        });
                    $('#checkflightdate').click(function () {
                        if ($('#checkflightdate').is(':checked')) {
                            $("#checkflightdate").val();
                            $("#checkmasterdate").prop("checked", false);
                            //alert("Checkbox is checked.");
                        }
                    });

                    //DAILY REPORT-DATE-ITEM-GROUP WISE(GROUP BY ITEM) 
                    if (reportid == "7") { 

                        $('#cargotypeallitemcheck').click(function () {
                            if ($(this).is(':checked')) {
                                $('#cargotypefromitem').prop('readonly', true);
                                $('#cargotypefromitem').val('');
                                $('#cargotypetoitem').prop('readonly', true);
                                $('#cargotypetoitem').val('');
                                // alert("Checkbox is checked.");
                            }
                            else {

                                $('#cargotypefromitem').prop('readonly', false);
                                $('#cargotypetoitem').prop('readonly', false);
                                $('#cargotypefromitem').val('');
                                $('#cargotypetoitem').val('');
                              
                            }
                        });

                        $('#allgroupcheck').click(function () {
                            if ($(this).is(':checked')) {
                                $('#groupfromitem').prop('readonly', true);
                                $('#groupfromitem').val('');
                                $('#grouptoitem').prop('readonly', true);                             
                                $('#grouptoitem').val('');
                                // alert("Checkbox is checked.");
                            }
                            else {

                                $('#groupfromitem').prop('readonly', false);
                                $('#groupfromitem').val('');

                                $('#grouptoitem').prop('readonly', false);
                                $('#grouptoitem').val('');
                            }
                        });
                      

                        $("#datewise").show();
                        //$("#mawbtextbox").show();
                        $("#freightpaymode").hide()
                        $("#cargotypeitemwise").show();
                        $("#destinationwise").hide();
                        $("#freighterwise").hide();
                        $("#groupwise").show();
                        $("#continentwise").hide();
                        $("#agentwise").hide();
                        $("#btnsend").hide();
                        //pdfExportCheck();

                    }
                    //DAILY REPORT-DATE-ITEM-GROUP WISE(WITHOUT GROUP) 
                    else if (reportid == "8") {

                        $('#cargotypeallitemcheck').click(function () {
                            if ($(this).is(':checked')) {
                                $('#cargotypefromitem').prop('readonly', true);
                                $('#cargotypetoitem').prop('readonly', true);

                                // alert("Checkbox is checked.");
                            }
                            else {

                                $('#cargotypefromitem').prop('readonly', false);
                                $('#cargotypetoitem').prop('readonly', false);
                            }
                        });
                        //if ($(allgroupcheck).is(":checked")) {
                        //    $('#fromgroup').prop('readonly', true);
                        //    $('#togroup').prop('readonly', true);

                        //    // alert("Checkbox is checked.");
                        //}
                        //else {
                        //    $('#fromgroup').prop('readonly', false);
                        //    $('#togroup').prop('readonly', false);
                        //}

                        $("#datewise").show();
                        //$("#mawbtextbox").show();
                        $("#freightpaymode").hide()
                        $("#cargotypeitemwise").show();
                        // $("#cargotypeitemwisewihout").show();
                        $("#groupwise").hide();
                        $("#freighterwise").hide()
                        $("#agentwise").hide();
                        $("#continentwise").hide();
                        $("#btnsend").hide();

                    }
                   // reportName == "DAILY REPORT-DATE-FREIGHTER-GROUP WISE(GROUP BY FREIGHTER)"
                    else if (reportid == "9") {

                        $('#freightertypeallitemcheck').click(function () {
                            if ($(this).is(':checked')) {
                                $('#freightertypefromitem').prop('readonly', true);
                                $('#freightertypefromitem').val('');
                                $('#freightertypetoitem').prop('readonly', true);
                                $('#freightertypetoitem').val('');

                                // alert("Checkbox is checked.");
                            }
                            else {

                                $('#freightertypefromitem').prop('readonly', false);
                                $('#freightertypefromitem').val('');
                                $('#freightertypetoitem').prop('readonly', false);
                                $('#freightertypetoitem').val('');
                            }
                        });

                        $('#allgroupcheck').click(function () {
                            if ($(this).is(':checked')) {
                                $('#groupfromitem').prop('readonly', true);
