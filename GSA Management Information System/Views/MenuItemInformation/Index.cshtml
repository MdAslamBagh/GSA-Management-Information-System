@model IEnumerable<GSA_Management_Information_System.Models.MenuItemInformation>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Menu_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleIcon)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Menu_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleIcon)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MenuItemId }) |
            @Html.ActionLink("Details", "Details", new { id=item.MenuItemId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MenuItemId })
        </td>
    </tr>
}

</table>
